{
  "version": 2,
  "name": "bhabit-cbmooners-frontend",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://billyjean-backend-5032385663.us-central1.run.app/api/$1"
    }
  ],
  "build": {
    "env": {
      "VITE_API_URL": "https://billyjean-backend-5032385663.us-central1.run.app"
    }
  },
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite"
}
